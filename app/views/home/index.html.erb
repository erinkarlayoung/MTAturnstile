<div id="map_daddy" class="map">
	<p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
	<%= render 'home/map_image' %>

<% @station.each do |station| %>
		<div id="<%= station.id %>" style="display:none" class="station_summary container">
			<h3><%= station.name %></h3>
			<p><%= station.borough %></p>
			<p><%= station.neighborhood %></p>
			<p><%= station.train %></p>
		</div>
	<% end %>